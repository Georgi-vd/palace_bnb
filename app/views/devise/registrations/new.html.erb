<div class="test123">
  <img class="mb-4" src="https://getbootstrap.com/docs/4.4/assets/brand/bootstrap-solid.svg" alt="" width="72" height="72">
  <p class="h5 mb-2 font-weight-normal font-weight-bold">Bienvenue ðŸ‘‹</p>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <div class="form-inputs form-signin">
      <%= f.text_field :first_name, required: true, autofocus: true, class: 'form-control forms-coller', placeholder: "First name" %>
      <%= f.text_field :last_name, required: true, class: 'form-control forms-coller', placeholder: "Last name" %>
      <%= f.email_field :email, required: true, class: 'form-control forms-coller', placeholder: "Email adress", input_html: { autocomplete: "email" } %>
      <%= f.password_field :password, required: true, class: 'form-control forms-coller', placeholder: "Password", input_html: { autocomplete: "new-password" } %>
      <%= f.password_field :password_confirmation, required: true, class: 'form-control', placeholder: "Password confirmation", input_html: { autocomplete: "new-password" } %>

      <div class="form-actions py-3">
        <%= f.submit "Sign up", autocomplete: "new-password", class: 'btn btn-lg btn-dark btn-block rounded-15' %>
      </div>
    </div>
  <% end %>
  <%= link_to "Log in", new_user_session_path, class: 'text-muted' %>
</div>
